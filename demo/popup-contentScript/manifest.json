
{
  "name": "test-popup-contentScripts",
  "version": "1.0",
  "manifest_version": 3,
  "description": "测试popup和contentScripts通信",
  "action": {
    "default_title": "打开配置",
    "default_popup": "html/index.html"
  },


  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-3.5.1.min.js", "js/contentScripts.js"]
    }
  ]

}
